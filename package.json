{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "yarn tsc",
    "format": "yarn prettier --write '**/*.ts'",
    "format-check": "yarn prettier --check '**/*.ts'",
    "lint": "yarn eslint src/**/*.ts",
    "dist": "yarn build && yarn ncc build --source-map --license licenses.txt",
    "test": "yarn jest",
    "all": "yarn build && yarn format && yarn lint && yarn test && yarn dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Sentry",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@actions/artifact": "^0.6.1",
    "@actions/core": "^1.6.0",
    "github-fetch-workflow-artifact": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.7.0",
    "eslint-config-sentry": "^1.73.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.4.6",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.1",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
